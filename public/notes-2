"build" = "nest build",
"prestart" = "npm run migrations:generate && npm run migrations:run && npm run seed:run",
"start" = "node dist/src/app",
"start:dev" = "nest start --watch",
"start:debug" = "nest start --debug --watch",
"typeorm-check-dist" = "node -e \"require('fs').existsSync('./dist') || require('child_process').execSync('npm run build', { stdio = 'inherit' });\"",
"typeorm" = "nest build && typeorm-ts-node-commonjs -d dist/src/config/orm.config",
"typeorm-extenstion" = "nest build && ts-node ./node_modules/typeorm-extension/bin/cli.cjs -d dist/src/config/orm.config",
"typeorm-extenstion-seed" = "nest build && ts-node ./node_modules/typeorm-extension/bin/cli.cjs -d dist/src/config/seeder.config",
"db:drop" = "npm run typeorm-extenstion db:drop",
"db:create" = "npm run typeorm-extenstion db:create",
"db:reset" = "npm run typeorm schema:drop",
"db:sync" = "npm run typeorm schema:sync",
"migrations:generate" = "npm run typeorm migration:generate src/core/migrations/migrations",
"migrations:revert" = "npm run typeorm migration:revert",
"migrations:clear" = "rimraf src/core/migrations ",
"migrations:run" = "npm run typeorm migration:run",
"seed:run" = "npm run typeorm-extenstion-seed seed:run",
"format" = "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
"lint" = "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
"test" = "jest",
"test:watch" = "jest --watch",
"test:cov" = "jest --coverage",
"test:debug" = "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"